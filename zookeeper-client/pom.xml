  <packaging>pom</packaging>
  <name>Apache ZooKeeper - Client</name>
  <description>ZooKeeper client</description>

  <profiles>
    <profile>
      <id>full-build</id>
      <modules>
        <module>zookeeper-client-c</module>
      </modules>
    </profile>
    <profile>
      <id>java-build</id>
      <modules />
